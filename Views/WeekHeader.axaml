        <TextBlock Text="一"></TextBlock>
        <TextBlock Text="二"></TextBlock>
        <TextBlock Text="三"></TextBlock>
        <TextBlock Text="四"></TextBlock>
        <TextBlock Text="五"></TextBlock>
        <TextBlock Text="六"></TextBlock>
        <TextBlock Text="日"></TextBlock>
    </StackPanel>
</UserControl>
